{
    "manifest_version": 3,
    "name": "YoutubeRead",
    "version": "1.0",
    "description": "A simple extension to summarize Youtube videos",
    "permissions": ["scripting", "activeTab", "tabs"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "climb.png",
        "48": "climb.png",
        "128": "climb.png"
      }
    }
  }
  