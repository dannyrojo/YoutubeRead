{
    "manifest_version": 3,
    "name": "Simple Extension",
    "version": "1.0",
    "description": "A simple browser extension with radio buttons",
    "permissions": ["scripting", "activeTab"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "climb.png",
        "48": "climb.png",
        "128": "climb.png"
      }
    }
  }
  